{
    "compilerOptions": {
        "target": "ES2016",
        "rootDir": "src",
        "outDir": "build",
        "module": "CommonJS",
        "moduleResolution": "node",
        "noEmitOnError": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "lib": ["es6"]
    }
}
